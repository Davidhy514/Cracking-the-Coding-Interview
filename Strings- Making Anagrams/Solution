def number_needed(a, b):
    pass
    ls = [0] * 26
    num = 0
    for c in a:
        ls[ord(c) - ord('a')] += 1
    for c in b:
        ls[ord(c) - ord('a')] -= 1
    for i in ls:
        num += abs(i)
        
    return (num)
    
a = input().strip()
b = input().strip()

print(number_needed(a, b))
